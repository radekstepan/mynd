<td class="check"><input type="checkbox" <% if @row["selected"]: %>checked="checked"<% end %> /></td>
<td class="description">
    <%= @row["description"] %>
    <% if @row["externalLink"]: %>
        [<a href="<%- @row["externalLink"] %>" target="_blank">Link</a>]
    <% end %>
</td>
<td class="pValue"><%= @row["p-value"].toPrecision(5) %></td>
<td class="matches">
    <a class="count" style="cursor:pointer"><%= @row["matches"] %></a>
</td>